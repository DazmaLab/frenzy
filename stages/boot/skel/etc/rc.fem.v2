#!/bin/sh

femfs="/home/technix/tmp/dupdir/femfs"
femdir="/home/technix/tmp/dupdir/femdir"

    ##if [ "$nonexist" = "YES" ]; then
    for pkfile in `cd ${femfs} && find . -xdev | sed "s/\.\///"g`
    do
    if [ -d ${femfs}/${pkfile} ]; then
    mkdir ${femdir}/${pkfile}
    fi
    if [ -f ${femfs}/${pkfile} ]; then
    ln -s ${femfs}/${pkfile} ${femdir}/${pkfile}
    fi
    #if [ -L ${femfs}/${pkfile} ]; then
    #cp ${femfs}/fs/${pkfile} ${femdir}/${pkfile}
    #fi
    done    

